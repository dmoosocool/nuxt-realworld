{
  "name": "nuxt-realworld",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .stylelintignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "axios": "^0.24.0",
    "consola": "^2.15.3",
    "cookieparser": "^0.1.0",
    "dayjs": "^1.8.28",
    "defu": "^5.0.0",
    "js-cookie": "^3.0.1",
    "markdown-it": "^12.2.0",
    "node-fetch": "^3.0.0",
    "nuxt": "^2.12.2",
    "pm2": "^5.1.0",
    "ufo": "^0.7.5",
    "vue": "^2.6.14",
    "vue-client-only": "^2.1.0",
    "vue-meta": "^2.4.0",
    "vue-no-ssr": "^1.1.1",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.1",
    "babel-eslint": "^10.1.0",
    "commitlint": "^13.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "5.58.2"
  }
}
